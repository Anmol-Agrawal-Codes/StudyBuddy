{
    "version": 2,
    "builds": [
        {
            "src": "STUDYBUD/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.12"
            }
        },
        {
            "src": "build.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDit": "staticfiles"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/{.*}",
            "dest": "/static/$1"
        },
        {
            "src": "/{.*}",
            "dest": "STUDYBUD/wsgi.py"
        }
    ]
}